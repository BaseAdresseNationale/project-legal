{
  "name": "@etalab/project-legal",
  "version": "0.2.1",
  "description": "Projection de coordonnées WGS-84 vers le système légal français en vigueur",
  "main": "./lib/index.js",
  "repository": "git@github.com:etalab/project-legal.git",
  "author": "Jérôme Desboeufs <jerome.desboeufs@data.gouv.fr>",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib",
    "browser",
    "projections.json"
  ],
  "scripts": {
    "test": "xo && ava",
    "transpile": "babel lib --out-dir browser",
    "prepublishOnly": "yarn transpile"
  },
  "dependencies": {
    "proj4": "^2.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "ava": "^0.25.0",
    "xo": "^0.23.0"
  },
  "xo": {
    "semicolon": false,
    "space": 2
  },
  "engines": {
    "node": ">= 8"
  },
  "browser": {
    "./index.js": "./browser/index.js"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread"
    ],
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              "> 1% in FR",
              "last 2 versions",
              "ie 11",
              "Firefox ESR"
            ]
          }
        }
      ]
    ]
  }
}

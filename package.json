{
  "name": "@etalab/project-legal",
  "version": "0.3.3",
  "description": "Projection de coordonnées WGS-84 vers le système légal français en vigueur",
  "main": "./index.js",
  "repository": "git@github.com:etalab/project-legal.git",
  "author": "Jérôme Desboeufs <jerome.desboeufs@data.gouv.fr>",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib",
    "browser",
    "index.js",
    "browser.js",
    "projections.json"
  ],
  "scripts": {
    "test": "xo && ava",
    "transpile": "babel lib --out-dir browser",
    "prepublishOnly": "yarn transpile"
  },
  "dependencies": {
    "proj4": "^2.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "ava": "^0.25.0",
    "xo": "^0.23.0"
  },
  "xo": {
    "semicolon": false,
    "space": 2
  },
  "engines": {
    "node": ">= 8"
  },
  "browser": {
    "./index.js": "./browser.js"
  },
  "browserslist": [
    "> 1% in FR",
    "last 2 versions",
    "ie 11",
    "Firefox ESR"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "@babel/plugin-transform-runtime",
        {
          "corejs": 2,
          "helpers": true
        }
      ]
    ]
  }
}
